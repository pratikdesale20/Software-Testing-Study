package StringBuffer_StringBuilder;

public class StringBuffer_append {

	public static void main(String[] args) {
		
		
		StringBuffer sb=new StringBuffer("selenium ");
		
		sb.append("Automation Testing Tool ");
		
		System.out.println(sb);
		
		
		sb.append('P');
		System.out.println(sb);
		
		sb.append( 2020);
		
		System.out.println(sb);
		
		
		
		
		
	}

}
